    [4, -1, -1, 1, 5, -1, -1, -1, 3],
    [-1, -1, 2, 7, -1, 4, -1, 1, -1],
    [-1, 8, -1, -1, 9, -1, 6, -1, -1],
    [-1, 7, -1, -1, -1, 6, 3, -1, -1],
    [-1, 3, -1, -1, 7, -1, -1, 8, -1],
    [9, -1, 4, 5, -1, -1, -1, -1, 1],
  ],
  "solution": [
    [2, 5, 6, 8, 3, 7, 1, 4, 9],
